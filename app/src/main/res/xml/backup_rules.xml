<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!--
        DarkTunnel VPN Backup Rules
        ===========================
        Exclude sensitive data from backups
    -->
    
    <!-- Exclude encrypted preferences -->
    <exclude domain="sharedpref" path="darktunnel_secure_prefs.xml"/>
    
    <!-- Exclude any credential files -->
    <exclude domain="file" path="credentials/"/>
    
    <!-- Exclude profiles directory -->
    <exclude domain="file" path="profiles/"/>
    
    <!-- Include regular preferences only -->
    <include domain="sharedpref" path="darktunnel_regular_prefs.xml"/>
    
</full-backup-content>
